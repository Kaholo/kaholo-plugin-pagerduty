{
  "name": "pager-duty",
  "viewName": "PagerDuty",
  "type": "executer",
  "execProgram": "node",
  "main": "app.js",
  "exposeRoute": true,
  "imgUrl": "pg.png",
  "version": "1.0.2",
  "description": "Integration with PagerDuty REST API integration",
  "keywords": [
    "pagerduty",
    "incident",
    "event",
    "api"
    ],
  "settings": [
    {
      "name": "TOKEN",
      "viewName": "Token",
      "type": "vault"
    }
  ],
  "methods": [
    {
      "name": "CREATE_INCIDET",
      "viewName": "Create Incident",
      "params": [
        {
          "name": "EMAIL",
          "viewName": "Email",
          "description": "The email address of a valid user associated with the account making the request.",
          "type": "string"
        },
        {
          "name": "SERVICE_ID",
          "viewName": "Service ID",
          "type": "autocomplete",
          "autocompleteType": "function",
          "description": "The name of the service to create the incident in. You can find your PagerDuty Service ID by clicking on Service --> Service Directory --> Service Name. In the page URL, PagerDuty will give you the specific Service ID for that Service.",
          "functionName": "getAllServices",
          "required": true
        },
        {
          "name": "TITLE",
          "viewName": "Title",
          "description": "A succinct description of the nature, symptoms, cause, or effect of the incident.",
          "type": "string",
          "required": true
        },
        {
          "name": "ASSIGNEE",
          "viewName": "Assignee",
          "description": "Assign the incident to these assignees. Cannot be specified if an escalation policy is given.",
          "type": "autocomplete",
          "autocompleteType": "function",
          "functionName": "getUserList"
        }
      ]
    },
    {
      "name": "CREATE_CHANGE_EVENT",
      "viewName": "Create Change Event",
      "params": [
        {
          "name": "SUMMARY",
          "viewName": "Summary *",
          "type": "string",
          "required": true
        },
        {
          "name": "ROUTING_KEY",
          "viewName": "Routing Key *",
          "type": "string",
          "required": true
        },
        {
          "name": "SOURCE",
          "viewName": "Source",
          "type": "string"
        },
        {
          "name": "CUSTOM_DETAILS",
          "viewName": "Custom Details (An Object)",
          "type": "string"
        }
      ]
    }
  ],
  "category": "MONITORING"
}
